.about-us-brief {
  padding: 80px 0 0 0;
  max-width: 1000px;
  margin: 0 auto;
}

.about-us-brief p {
  text-align: center;
}

.about-us-brief h1 {
  margin-bottom: 30px;
}

.about-us-image-text {
  display: flex;
  margin: 120px 0 30px;
}

.about-us-image-text .about-us-text {
  flex: 1;
  display: flex;
  justify-content: center;
  align-items: center;
}

.about-us-image-text .about-us-text p {
  width: 360px;
}

.about-us-image-text .about-us-image {
  flex: 1;
}

.about-us-image-text .about-us-image img {
  width: 100%;
  max-height: 560px;
}

.about-us-quote {
  margin-top: 170px;
}

.about-us-quote q {
  font-size: 45px;
  text-align: center;
  font-family: var(--font--heading--family);
  max-width: 850px;
  display: block;
  margin: 0 auto;
  line-height: 50px;
  text-transform: uppercase;
  word-break: break-word;
  word-spacing: normal;
}

.about-us-quote .author {
  text-align: center;
  margin-top: 50px;
}

.about-us-eyebrows {
  margin-top: 150px;
  text-align: center;
}

@media (max-width: 768px) {
  .about-us {
    padding: 0 15px;
  }

  .about-banner {
    display: none;
  }

  .about-us-brief {
    margin-top: 40px;
    padding: 0;
  }

  .about-us-brief h1 {
    margin-bottom: 15px;
  }

  .about-us-image-text .about-us-image {
    order: 1;
    margin-left: -15px;
    margin-right: -15px;
  }

  .about-us-image-text {
    margin-top: 35px;
    display: flex;
    flex-direction: column;
  }

  .about-us-image-text .about-us-text {
    order: 2;
    margin-top: 45px;
    text-align: center;
  }

  .about-us-quote {
    margin-top: 80px;
  }

  .about-us-quote q {
    font-size: 28px;
    line-height: 28px;
  }

  .about-us-quote .author {
    margin-top: 20px;
  }

  .about-us-eyebrows {
    margin-top: 70px;
    margin-left: -15px;
    margin-right: -15px;
  }

  .about-us-eyebrows img {
    display: none;
  }

  .about-us-eyebrows {
    background: url(https://cdn.shopify.com/s/files/1/0531/2374/3901/files/About_eyelashes.png?v=1627932955);
    height: 370px;
    background-size: cover;
    background-position: center;
  }
}

@media (max-width: 414px) {
  .about-us-eyebrows {
    height: 220px;
  }
}

.locations {
  letter-spacing: 0;
  margin-bottom: 50px;
}

.locations-banner {
  width: 100%;
}

.locations-brief {
  padding: 80px 0 0 0;
  max-width: 900px;
  margin: 0 auto;
}

.locations-brief p {
  text-align: center;
  margin-bottom: 30px;
}

.locations-brief h1 {
  margin-bottom: 30px;
}

.locations-map {
  height: 640px;
  max-width: 1500px;
  margin: 80px auto 0;
  display: flex;
  justify-content: center;
}

.locations-find {
  border: 1px solid #ededed;
  padding: 40px;
  overflow: auto;
  max-width: 570px;
  flex: 1
}

.locations-google-map {
  flex: 2;
}

.find-location {
  margin: 20px 0 30px;
  max-width: 365px;
  overflow: auto;
  position: relative;
  background-image: url("data:image/svg+xml;utf8,<svg viewBox='0 0 20 20' xmlns='http://www.w3.org/2000/svg'><path fill='%23999' d='M14.37 12.86l5.636 5.637-1.414 1.414-5.633-5.632a7.627 7.627 0 01-4.688 1.604c-4.256 0-7.707-3.483-7.707-7.78 0-4.297 3.45-7.78 7.707-7.78s7.707 3.483 7.707 7.78c0 1.792-.6 3.442-1.608 4.758zM8.27 14.084c3.259 0 5.907-2.673 5.907-5.98 0-3.306-2.648-5.98-5.907-5.98-3.258 0-5.907 2.674-5.907 5.98 0 3.307 2.649 5.98 5.907 5.98z' fill-rule='nonzero' /></svg>");
  background-repeat: no-repeat;
  background-size: 20px;
  background-position: 10px 50%;
  padding-left: 40px;
}

.location-item {
  margin-top: 35px;
}

.location-item .name {
  font-size: 20px;
}

.location-item .details {
  margin-top: 15px;
  padding-left: 10px;
}

.location-item .address {
  background-image: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 512 512' style='enable-background:new 0 0 512 512;'><g fill='%23333'><path d='M256,0C153.755,0,70.573,83.182,70.573,185.426c0,126.888,165.939,313.167,173.004,321.035    c6.636,7.391,18.222,7.378,24.846,0c7.065-7.868,173.004-194.147,173.004-321.035C441.425,83.182,358.244,0,256,0z M256,278.719    c-51.442,0-93.292-41.851-93.292-93.293S204.559,92.134,256,92.134s93.291,41.851,93.291,93.293S307.441,278.719,256,278.719z'/></g></svg>");
  background-repeat: no-repeat;
  background-size: 18px;
  background-position: 0px 50%;
  padding-left: 25px;
}

.location-item .phone {
  margin-top: 5px;
  background-image: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 19.8 21.2' style='enable-background:new 0 0 19.8 21.2;'><g fill='%23333'><path d='M4.4,0.3c1.2-0.2,2,1.1,2.6,2.1c0.6,0.9,1.3,2,1,3.2C7.8,6.2,7.2,6.6,6.8,7C6.3,7.3,5.6,7.7,5.5,8.3   c-0.3,1,0.3,2,0.7,2.6C7,12.2,8,13.3,9.3,14.4c0.6,0.5,1.5,1.2,2.4,1c1.3-0.3,1.6-1.9,3-2.1c1.3-0.2,2.2,0.8,3,1.4   c0.7,0.6,1.9,1.4,1.8,2.5c0,0.6-0.5,1-1,1.4c-0.4,0.4-0.8,0.8-1.3,1.1c-1.1,0.7-2.3,1-3.8,1c-1.5,0-2.6-0.5-3.7-1.1   c-2.1-1.1-3.7-2.6-5.2-4.3C3.1,13.6,1.7,11.6,1,9.4C0,6.6,0.5,3.8,2.1,2c0.3-0.3,0.7-0.6,1.1-0.9C3.5,0.8,3.8,0.4,4.4,0.3z'/></g></svg>");
  background-repeat: no-repeat;
  background-size: 15px;
  background-position: 0px 50%;
  padding-left: 25px;
}

.location-item .phone a {
  text-decoration: none;
}

.location-item .distance {
  margin-top: 5px;
  background-image: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' xmlns:xlink='http://www.w3.org/1999/xlink' viewBox='0 0 500 500'><g id='left'><path d='m30 370v69c0 8.864 7.136 16 16 16h40c8.864 0 16-7.136 16-16v-69z' /><path d='m100.5 40c-11.609 0-21.516 7.014-25.781 17.031l-72.5 170c-1.4364 3.37-2.219 7.07-2.219 10.97v94c0 15.512 12.488 28 28 28h223v-175.12l-192.91 0.53125c12.756-32.024 38.281-96.094 38.281-96.094 4.788-12.02 5.115-13.631 12.031-13.625l142.6 0.123v-35.813h-150.5z' /><path style='fill:%23FFF' d='m100 270a35 35 0 0 1 -70 0 35 35 0 1 1 70 0z' /></g><use xlink:href='%23left' transform='matrix(-1,0,0,1,500,0)' /></svg>");
  background-repeat: no-repeat;
  background-size: 15px;
  background-position: 0px 50%;
  padding-left: 25px;
}

.location-item .openhours-label {
  margin-top: 5px;
  background-image: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 24 24' fill='none' stroke='currentColor' stroke-width='2' stroke-linecap='round' stroke-linejoin='round'><circle cx='12' cy='12' r='10'></circle><polyline points='12 6 12 12 16 14'></polyline></svg>");
  background-repeat: no-repeat;
  background-size: 15px;
  background-position: 0px 50%;
  padding-left: 25px;
}

.location-item .openhours-label:after {
  content: '';
  background-image: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 10 6'><path d='M5 1.942l-3.53 3.53-.707-.707 3.53-3.53L5 .528l4.237 4.237-.707.707L5 1.942z' fill-rule='nonzero' /></svg>");
  background-repeat: no-repeat;
  background-size: 15px;
  background-position: 100% 50%;
  padding-right: 25px;
}

.location-item .openhours-label.collapsed:after {
  background-image: url("data:image/svg+xml;utf8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 10 6'><path d='M5 4.058L8.53.528l.707.707-3.53 3.53L5 5.472.763 1.235 1.47.528 5 4.058z' fill-rule='nonzero' /></svg>");
}

.location-item .openhours-list {
  margin-top: 5px;
  list-style: none;
  padding: 0;
  transition: all 0.2s ease-in;
  max-height: 150px;
  overflow: hidden;
}

.location-item .openhours-label.collapsed + .openhours-list {
  max-height: 0px;
}

.location-item .openhours-list > li {
  display: flex;
  line-height: 20px;
}

.location-item .openhours-list > li span:first-child {
  width: 110px;
}

.location-item .openhours-list > li span:last-child {
  width: 65px;
  text-align: right;
}

.location-item .actions {
  margin-top: 15px;
  text-transform: uppercase;
}

.location-item .actions > a {
  margin-right: 25px;
}

@media (max-width: 768px) {
  .locations {
    padding: 0 15px;
    margin-bottom: 0;
  }

  .locations-banner {
    display: none;
  }

  .locations-brief {
    margin-top: 40px;
    padding: 0;
  }

  .locations-brief h1 {
    margin-bottom: 15px;
  }

  .locations-brief p {
    margin-bottom: 15px;
  }

  .locations-map {
    flex-direction: column;
    position: relative;
    margin: 0;
    justify-content: flex-start;
    height: auto;
    padding: 0 15px;
  }

  .locations-find {
    border: 0px;
    padding: 0;
    overflow: visible;
    max-width: none;
  }

  .find-location {
    height: 50px;
    max-width: none;
  }

  .locations-google-map {
    flex: auto;
    height: 290px;
    position: absolute !important;
    top: 110px;
    left: 0;
    right: 0;
  }

  .locations-search-result {
    padding-top: 300px;
  }

  .location-item {
    padding-bottom: 30px;
    border-bottom: 1px solid #ededed;
    margin-left: -15px;
    margin-right: -15px;
  }

  .location-item .details {
    padding: 0 15px;
  }

  .location-item .name {
    padding: 0 15px;
  }

  .location-item .actions {
    padding: 0 15px;
  }
}

@media (max-width: 414px) {
  .location-item .actions {
    display: flex;
    justify-content: space-between;
  }

  .location-item .actions > a {
    margin-right: 0;
  }
}

/* Treatment page */
.treatment_page_banner img {
  width: 100%;
}
.container {
  max-width: 1000px;
  margin: 0 auto;
  text-align: center;
}
.template-page-title h1 {
  margin: 60px 0 30px;
}
.cms-content p {
  margin-bottom: 25px;
}
.treatment-book-items {
  overflow: hidden;
  margin-bottom: 70px;
}
.treatment-book-items .treatment-book-item {
  float: left;
  width: 50%;
  padding: 0 30px;
}
.treatment-book-items .treatment-book-item .treatment-book-top {
  border-bottom: 1px solid #454545;
  text-align: left;
  margin-bottom: 5px;
  padding-bottom: 5px;
  overflow: hidden;
}
.treatment-book-items .treatment-book-item .treatment-book-top .treatment-book-title {
  float: left;
}
.treatment-book-items .treatment-book-item .treatment-book-top .treatment-book-price {
  float: right;
}
.treatment-book-items .treatment-book-item .treatment-book-description {
  text-align: left;
}
.treatment-book-items .treatment-book-item .treatment-book-link a {
  background: #454545;
  color: #fff;
  display: block;
  line-height: 45px;
  text-transform: uppercase;
  font-size: 14px;
}
.content-expert h2 {
  margin-bottom: 15px;
}
.content-expert .content_expert_item {
  margin-bottom: 60px;
}
.featured-content--root.featured-content-images[data-text-position="right"][data-overlap="false"] .featured-content--text-container {
  max-width: 500px;
}
.featured-content--root.featured-content-images .featured-content--title {
  font-style: normal;
  line-height: 1;
}
.treatment-page-section .feature-video--thumbnail img {
  width: 100%;
}
.content-expert.content-expert-wrapper {
  margin-top: 60px;
}
@media (max-width: 767px) {
  .treatment-book-items .treatment-book-item {
    width: 100%;
    padding: 0 15px;
    margin-bottom: 30px;
  }
  .featured-content--root.featured-content-images .featured-content--title {
    font-size: 30px;
  }
  .content-expert.content-expert-wrapper {
    margin-top: 40px;
    padding: 0 15px;
  }
  .content-expert .content_expert_item {
    margin-bottom: 40px;
  }

}


