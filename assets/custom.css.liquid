.promo-content .announcement-bar {
  position: absolute;
  top: 100%;
  left: 0;
  width: 100%;
  height: 40px;
  line-height: 40px;
  opacity: 1;
  -webkit-animation: 16s announcementRotate infinite ease-in-out;
  -moz-animation: 16s announcementRotate infinite ease-in-out;
  -o-animation: 16s announcementRotate infinite ease-in-out;
  animation: 16s announcementRotate infinite ease-in-out;
  animation-delay: 0s;
}
@keyframes announcementRotate {
  0% {top: 100%}
  4% {top: 0%}
  25% {top: 0%}
  26% {top: -100%;}
  100% {top: -100%;}
}
.promo-content .announcement-bar:nth-child(1) {
  animation-delay: 0s;
}
.promo-content .announcement-bar:nth-child(2) {
  animation-delay: 4s;
}
.promo-content .announcement-bar:nth-child(3) {
